{
  "somesass.scannerExclude": [
    "**/.git/**",
    "**/node_modules/**",
    "**/bower_components/**"
  ],
  "somesass.scss.completion.suggestAllFromOpenDocument": false,
  "somesass.scss.completion.suggestFromUseOnly": false,
  "somesass.scss.completion.suggestionStyle": "all",
  "somesass.scss.completion.suggestFunctionsInStringContextAfterSymbols": " (+-*%",

  "somesass.sass.completion.suggestAllFromOpenDocument": true,
  "somesass.sass.completion.suggestFromUseOnly": false,
  "somesass.sass.completion.suggestionStyle": "all",
  "somesass.sass.completion.suggestFunctionsInStringContextAfterSymbols": " (+-*%"
}
