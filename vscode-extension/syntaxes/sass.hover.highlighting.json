{
	"information_for_contributors": [
		"This file is a local copy of https://github.com/TheRealSyler/vscode-sass-indented/blob/master/syntaxes/sass.hover.highlighting.json",
		"If you want to provide a fix or improvement, please create a pull request against the original repository.",
		"Once accepted there, we are happy to receive an update request."
	],
	"version": "https://github.com/TheRealSyler/vscode-sass-indented/commit/b2b24cc2e82168fd7f8ba9caad9cfd4f080c6e19",
  "fileTypes": ["sass.hover"],
  "name": "Sass Hover",
  "patterns": [
    {
      "match": "(Path)",
      "name": "entity.name.function"
    },
    {
      "match": "([A-z\\/-]+): (.*)",
      "name": "meta.path",
      "captures": {
        "1": {
          "name": "variable"
        },
        "2": {
          "name": "entity.name.type"
        }
      }
    },
    {
      "match": "([A-z\\/-]+)",
      "name": "meta.path",
      "captures": {
        "1": {
          "name": "keyword.control"
        }
      }
    },
    {
      "begin": "\\.([A-z-]+)",
      "end": "\\n|[\\t ]",
      "name": "meta.extension",
      "captures": {
        "1": {
          "name": "keyword.other"
        }
      }
    }
  ],

  "scopeName": "source.sass.hover"
}
